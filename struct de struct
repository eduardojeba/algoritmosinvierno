// struct.cpp: define el punto de entrada de la aplicaci√≥n de consola.
//

#include "stdafx.h"
#include "conio.h"
#include <string>
#include <iostream>
#define max 30

using namespace std;


struct tvector{

	string vec[max];
};
struct gvector{

	string vector[max];
};

void cargarVectordeVectores(tvector v[], int n);
void mostrarMatriz(tvector v[], tvector mat[max][max], int n);



void main(){

	tvector b[max];
	gvector matriz[max][max];
    int n;
	cout<<"ingrese n: ";
	cin>>n;
	cin.ignore();
	cargarVectordeVectores(b,n);
	mostrarMatriz(b,matriz,n);
	getch();

}


void cargarVectordeVectores(tvector v[], int n){

	for (int i=0;i<n;i++){
	
		for(int j=0;j<n;j++){
		
			cout<<endl<<"Ingrese un elemento: ";
			
			getline(cin,v[i].vec[j]);
		}
	}


}

void mostrarMatriz(tvector v[], gvector mat[max][max], int n){

	int i,j,k;
	for(i=0;i<n;i++){
		for(j=0;j<n;j++){
			for(k=0;k<n;k++){
				(mat[i][j].vector[k]) = ( v[i].vec[j]);
				cout<<mat[i][j].vector[k]<<"  ";
			
			}
			
		}
		cout<<endl;
	}
}

